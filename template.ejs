<html>
  <head>
    <title><%= htmlWebpackPlugin.options.title %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link href="https://fonts.googleapis.com/css?family=Viga&display=swap" rel="stylesheet">
    <link href="/styles/index.css" type="text/css" rel="stylesheet">

    <link rel="icon" type="image/png" href="/assets/favicon-32x32.png" sizes="32x32" />
    <link rel="icon" type="image/png" href="/assets/favicon-16x16.png" sizes="16x16" />
  </head>

  <body>
    <main>
      <div id="passport-color">
      </div>
      <div id="destination-color">
      </div>

      <div class="search-wrapper">
          <label id="prompt"></label>
          <div class="typeahead-wrapper">
            <input id="typeahead" />
          </div>
          <div id="options-wrapper"></div>
        </div>

      <div class="countries">
        <div class="passport-wrapper">
            <div class="passport">
              <label>Passport</label>
              <div id="passport-name"></div>
              <div id="passport-code"></div>
            </div>
          </div>
    
          <div class="destination-wrapper">
            <div class="destination">
              <label>Destination</label>
              <div id="destination-name"></div>
              <div id="destination-code"></div>
          </div>
        </div>
      </div>

      <div class="visa-info-wrapper">
        <div class="header-wrapper">
          <div id="header"></div>
        </div>

        <div class="visa-info">
          <div class="requirement-wrapper">
            <div id="requirement"></div>
          </div>
          <div class="allowed-stay-wrapper">
            <div id="allowed-stay"></div>
          </div>        
          
          <div class="notes-wrapper">
            <div id="notes"></div>
          </div>

          <div class="sources-wrapper">
            <div id="sources"></div>
          </div>
        </div>
      </div>

      <div class="site-info">
        <div id="seo-content">
          <h1>Do I need a visa? <br />Can I use an eVisa?</h1>
          <h2>
            <%= htmlWebpackPlugin.options.seoContent %>
          </h2>
        </div>
        <div>Created by <a href="https://christinecha.com" target="_blank">Christine Cha</a></div>
      </div>
    </main>

    <script>
      window.INITIAL_PASSPORT = "<%= htmlWebpackPlugin.options.INITIAL_PASSPORT %>"
    </script>

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-142382461-1"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-142382461-1');
    </script>

  </body>
</html>